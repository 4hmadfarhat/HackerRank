package challanges;

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Array2D {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		int arr[][] = new int[6][6];
		int sum1=0;
		int sum2=0;
		int sum3=0;
		int totalSum =0;
		int maxSum = 0;
		
		for(int arr_i=0; arr_i < 6; arr_i++){
			for(int arr_j=0; arr_j < 6; arr_j++){
				arr[arr_i][arr_j] = in.nextInt();
				//1------------------------------------------------------------------------------------------
				if ((arr_j>=0 && arr_j<=2) && arr_i==0)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==1 && arr_j==1)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=0 && arr_j<=2)&&arr_i==2)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=1 && arr_j<=3) && arr_i==0)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==1 && arr_j==2)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=1 && arr_j<=3)&&arr_i==2)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=2 && arr_j<=4) && arr_i==0)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==1 && arr_j==3)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=2 && arr_j<=4)&&arr_i==2)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=3 && arr_j<=5) && arr_i==0)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==1 && arr_j==4)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=3 && arr_j<=5)&&arr_i==2)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				//2------------------------------------------------------------------------------------------

				if ((arr_j>=0 && arr_j<=2) && arr_i==1)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==2 && arr_j==1)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=0 && arr_j<=2)&&arr_i==3)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=1 && arr_j<=3) && arr_i==1)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==2 && arr_j==2)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=1 && arr_j<=3)&&arr_i==3)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=2 && arr_j<=4) && arr_i==1)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==2 && arr_j==3)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=2 && arr_j<=4)&&arr_i==3)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=3 && arr_j<=5) && arr_i==1)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==2 && arr_j==4)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=3 && arr_j<=5)&&arr_i==3)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				//3------------------------------------------------------------------------------------------

				if ((arr_j>=0 && arr_j<=2) && arr_i==2)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==3 && arr_j==1)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=0 && arr_j<=2)&&arr_i==4)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=1 && arr_j<=3) && arr_i==2)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==3 && arr_j==2)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=1 && arr_j<=3)&&arr_i==4)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=2 && arr_j<=4) && arr_i==2)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==3 && arr_j==3)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=2 && arr_j<=4)&&arr_i==4)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=3 && arr_j<=5) && arr_i==2)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==3 && arr_j==4)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=3 && arr_j<=5)&&arr_i==4)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				//4------------------------------------------------------------------------------------------

				if ((arr_j>=0 && arr_j<=2) && arr_i==3)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==4 && arr_j==1)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=0 && arr_j<=2)&&arr_i==5)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=1 && arr_j<=3) && arr_i==3)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==4 && arr_j==2)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=1 && arr_j<=3)&&arr_i==5)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=2 && arr_j<=4) && arr_i==3)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==4 && arr_j==3)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=2 && arr_j<=4)&&arr_i==5)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;

				if ((arr_j>=3 && arr_j<=5) && arr_i==3)
				{
					sum1+=arr[arr_i][arr_j];
					if(arr_i==4 && arr_j==4)
					{
						sum2+=arr[arr_i][arr_j];
						if((arr_j>=3 && arr_j<=5)&&arr_i==5)
						{
							sum3+=arr[arr_i][arr_j];
						}   
					}
				}
				totalSum = sum1 + sum2 + sum3;
				if (totalSum >= maxSum)
					maxSum = totalSum;
				sum1=0;
				sum2=0;
				sum3=0;
				totalSum=0;



			}
		}
		System.out.println(maxSum);
		in.close();
	}
}
